{
    "name": "@ng2/common",
    "version": "1.4.0",
    "description": "Angular 2 module for common stuff",
    "main": "index.js",
    "typings": "index.d.ts",
    "scripts":
    {
        "prebuild": "typings install && gulp copy-tstypings",
        "build": "npm run tsc",
        "clean": "rimraf ./*.d.ts ./index.js ./index.dev.js ./index.min.js ./*.map ./*.log ./utils ./decorators ./types ./services ./components ./pipes",
        "clean:deps": "rimraf ./node_modules ./typings ./tsTypings",
        "clean:all": "npm run clean && npm run clean:deps",
        "tsc": "tsc",
        "tsc:w": "tsc -w --sourceMap",
        "tsc:debug": "tsc --sourceMap",
        "prepublish": "npm run bundle:package",
        "postpublish": "npm run clean",
        "republish": "npm publish --force",
        "bundle": "tsc --outFile ./index.dev.js",
        "bundle:update": "nab ./index.dev.js && uglifyjs -o ./index.min.js -- ./index.dev.js",
        "bundle:package": "npm run clean && npm run build && npm run bundle && npm run bundle:update",
        "deploy-stable": "npp -b",
        "deploy-next": "npp -p -b -s \"beta\" -t \"next\""
    },
    "repository":
    {
        "type": "git",
        "url": "git+https://gitlab.com/Buko/angular2-utilities.git"
    },
    "keywords": [
        "angular2",
        "ng2",
        "utilities"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "bugs":
    {
        "url": "https://gitlab.com/Buko/angular2-utilities/issues"
    },
    "homepage": "https://gitlab.com/Buko/angular2-utilities#README",
    "dependencies":
    {
        "@angular/core":  "2.0.0-rc.6",
        "@angular/common":  "2.0.0-rc.6",
        "@angular/compiler":  "2.0.0-rc.6",
        "@angular/platform-browser": "2.0.0-rc.6",
        "@angular/platform-browser-dynamic": "2.0.0-rc.6",
        "@angular/forms": "2.0.0-rc.6",
        "@angular/router":  "3.0.0-rc.2",
        "core-js": "~2.4.1",
        "reflect-metadata": "0.1.3",
        "zone.js": "0.6.17",
        "rxjs": "5.0.0-beta.11"
    },
    "peerDependencies":
    {
        "numeral": "~1.5.3"
    },
    "devDependencies":
    {
        "rimraf": "~2.5.4",
        "typescript": "1.8.10",
        "typings":"~1.3.2",
        "uglify-js": "~2.6.2",
        "gulp": "~3.9.1",
        "through2": "~2.0.1",
        "ng2-tstypings": "~1.1.1",
        "npm-publish-package": "~1.2.0",
        "npm-angular2-bundle": "~1.0.1"
    }
}