{
    "name": "ng2-utilities",
    "version": "1.0.0",
    "description": "Angular 2 module for utilities stuff",
    "scripts":
    {
        "build": "npm run tsc",
        "clean": "rimraf ./*.d.ts ./*.js ./*.map ./*.log",
        "clean:modules": "rimraf ./node_modules",
        "clean:all": "npm run clean && npm run clean:modules",
        "tsc": "tsc",
        "tsc:w": "tsc -w",
        "deployPatch": "npm run build && npp -b",
        "deployMinor": "npm run build && npp",
        "deployMajor": "npm run build && npp -m"
    },
    "repository":
    {
        "type": "git",
        "url": "git+https://gitlab.com/Buko/angular2-utilities.git"
    },
    "keywords": [
        "angular2",
        "ng2",
        "utilities"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "bugs":
    {
        "url": "https://gitlab.com/Buko/angular2-utilities/issues"
    },
    "homepage": "https://gitlab.com/Buko/angular2-utilities#README",
    "dependencies":
    {
        "angular2": "2.0.0-beta.1",
        "es6-promise": "^3.0.2",
        "es6-shim": "^0.33.3",
        "reflect-metadata": "0.1.2",
        "zone.js": "0.5.10",
        "rxjs": "5.0.0-beta.0"
    },
    "devDependencies":
    {
        "rimraf": "~2.5.1",
        "typescript": "^1.8.0",
        "npm-publish-package": "1.2.0"
    }
}