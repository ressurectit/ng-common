            "es2020": "./es2020/hotkeys/src/index.js",
            "es2015": "./es2015/hotkeys/src/index.js",
            "node": "./es2015/hotkeys/src/index.js",
            "default": "./es2020/hotkeys/src/index.js"
        },
        "./material": {
            "types": "./material/common-material.d.ts",
            "esm2020": "./es2020/material/src/index.js",
            "es2020": "./es2020/material/src/index.js",
            "es2015": "./es2015/material/src/index.js",
            "node": "./es2015/material/src/index.js",
