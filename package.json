        "rxjs": "^6.6.7",
        "@jscrpt/common": "^2.2.0",
        "angular2-hotkeys": "^2.4.0",
        "store": "^2.0.12",
        "structured-log": "^0.2.0",
        "moment": "^2.27.0",
        "date-fns": "^2.19.0",
        "positions": "^1.6.2",
