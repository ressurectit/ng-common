{
    "name": "ng2-common",
    "version": "1.0.1-beta.4",
    "description": "Angular 2 module for common stuff",
    "scripts":
    {
        "prebuild": "typings install",
        "build": "npm run tsc",
        "clean": "rimraf ./*.d.ts ./*.js ./*.map ./*.log ./bundle ./directives",
        "clean:deps": "rimraf ./node_modules ./typings",
        "clean:all": "npm run clean && npm run clean:deps",
        "tsc": "tsc",
        "tsc:w": "tsc -w --sourceMap",
        "tsc:debug": "tsc --sourceMap",
        "prepublish": "npm run bundle:package",
        "postpublish": "npm run clean",
        "bundle": "tsc --outFile bundle/ng2-common.dev.js",
        "bundle:update": "nab bundle/ng2-common.dev.js",
        "bundle:package": "npm run clean && npm run build && npm run bundle && npm run bundle:update",
        "deploy-next": "npp -p -b -s \"beta\" -t \"next\"",
        "deploy-current": "npm publish" 
    },
    "repository":
    {
        "type": "git",
        "url": "git+https://gitlab.com/Buko/angular2-utilities.git"
    },
    "keywords": [
        "angular2",
        "ng2",
        "utilities"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "bugs":
    {
        "url": "https://gitlab.com/Buko/angular2-utilities/issues"
    },
    "homepage": "https://gitlab.com/Buko/angular2-utilities#README",
    "dependencies":
    {
        "angular2": "2.0.0-beta.7",
        "es6-promise": "^3.0.2",
        "es6-shim": "^0.33.3",
        "reflect-metadata": "0.1.2",
        "zone.js": "0.5.15",
        "rxjs": "5.0.0-beta.2"
    },
    "devDependencies":
    {
        "rimraf": "~2.5.1",
        "typescript": "~1.8.2",
        "typings":"^0.6.8",
        "npm-publish-package": "1.2.0",
        "npm-angular2-bundle": "1.0.0"
    }
}